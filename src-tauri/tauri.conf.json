{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "starter-template",
  "version": "0.1.0",
  "identifier": "asd",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://127.0.0.1:3000",
    "beforeDevCommand": "",
    "beforeBuildCommand": "cd .. && trunk build --no-default-features -v --features csr && cp target/site/pkg/starter-template.css dist/ "
  },
  "app": {
    "windows": [
      {
        "title": "window-title",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}